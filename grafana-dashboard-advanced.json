{
  "dashboard": {
    "id": null,
    "title": "ðŸš€ TradingBot Advanced Monitoring Dashboard",
    "tags": ["tradingbot", "monitoring", "advanced", "trading"],
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "ðŸ¥ System Health Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "tradingbot_system_health",
            "legendFormat": "Overall Health"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "orange", "value": 50},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 90}
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "ðŸ’¾ Memory Usage (MB)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "tradingbot_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisLabel": "",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            }
          }
        },
        "yAxes": [
          {
            "label": "Memory (MB)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
